<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LockPatternView">
        <attr name="aspect" format="string" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_in_width" format="dimension" />
        <attr name="border_out_width" format="dimension" />
        <attr name="border_in_color" format="color" />
        <attr name="border_out_color" format="color" />
    </declare-styleable>
    <declare-styleable name="CircleProgressView">
        <attr name="background_stroke_width" format="dimension" />
        <attr name="background_color" format="color" />
        <attr name="progress_stroke_width" format="dimension" />
        <attr name="progress_color" format="color" />
        <attr name="indicator_stroke_width" format="dimension" />
        <attr name="indicator_color" format="color" />
        <attr name="indicator_radius" format="color" />
        <attr name="indicator_stroke_width1" format="dimension" />
        <attr name="indicator_color1" format="color" />
        <attr name="indicator_gap1" format="dimension" />
        <attr name="indicator_stroke_width2" format="dimension" />
        <attr name="indicator_color2" format="color" />
        <attr name="indicator_gap2" format="dimension" />
        <attr name="title_text_size" format="dimension" />
        <attr name="title_text_color" format="color" />
        <attr name="sub_text_size" format="dimension" />
        <attr name="sub_text_color" format="color" />
    </declare-styleable>
    <declare-styleable name="GifMoviewView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomTheme">
        <attr name="gifMoviewViewStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="CustomProgressBar">
        <attr name="progress_background_color" format="color" />
        <attr name="progress_top_color" format="color" />
        <attr name="progress_bottom_color" format="color" />
        <attr name="horizontal_margin" format="dimension" />
        <attr name="vertical_margin" format="dimension" />
    </declare-styleable>

</resources>